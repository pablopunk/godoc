{
  "version": 2,
  "name": "godoc",
  "alias": "godoc",
  "builds": [
    { "src": "index.js", "use": "@now/node" },
    { "src": "styles.css", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/styles.css", "dest": "/styles.css" },
    { "src": "/", "dest": "godoc.now.sh/pablopunk/godoc" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
